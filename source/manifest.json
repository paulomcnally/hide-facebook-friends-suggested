{
  "name": "Hide facebook friends suggested",
  "version": "0.0.2",
  "manifest_version" : 2,
  "description": "Hide facebook friends suggested",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "icons": {
    "16" : "icon-16.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "css" : ["style.css"]
    }
  ]
}
